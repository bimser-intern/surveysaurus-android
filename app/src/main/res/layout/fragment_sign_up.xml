        android:background="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_google" />

    <!-- Assistant image -->
    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="167dp"
